generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Application {
  id     Int    @id @default(autoincrement())
  key    String @unique
  secret String
}

model Agent {
  id            Int     @id @default(autoincrement())
  name          String
  salary        String
  currency      String
  department    String
  onContract    Boolean @default(false)
  subDepartment String
}
